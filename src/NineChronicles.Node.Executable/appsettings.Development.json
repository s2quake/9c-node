{
  "$schema": "./appsettings-schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Swarm": {
    "IsEnabled": true
  },
  "Validator": {
    "IsEnabled": true
  },
  "Action": {
    "ModulePath": "NineChronicles.Node.dll",
    "ActionLoaderType": "NineChronicles.Node.ActionLoader",
    "PolicyActionRegistryType": "NineChronicles.Node.PolicyActionRegistry"
  }
}
